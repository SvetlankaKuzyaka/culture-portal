	"bio": [
		{
			"date": "1904",
			"description": "She was born on 15 February, 1904 in Minsk."
		},
		{
			"date": "1920",
			"description": "Began to act as a singer in 1920 in the troupe of the theater Glavpolitprosvet of the Western Front."
		},
		{
			"date": "1924-1928",
			"description": "She studied at the Minsk Musical Technical School, in 1928 - in a special opera class."
		},
		{
			"date": "1927",
			"description": "She performed Belarusian folk songs at the International Music Fair in Frankfurt am Main (Germany)."
		},
		{
			"date": "1930-1933",
			"description": "She studied at the State Studio of Opera and Ballet (Minsk). Pupil V. A. Tsvetkov and A. P. Bonachich."
		},
		{
			"date": "1933",
			"description": "Soloist of the Belarusian Opera and Ballet Theater in Minsk. She performed both soprano and mezzo-soprano parts."
		},
		{
			"date": "1951—1960",
			"description": "She was the chief director of the Belarusian Opera and Ballet Theater."
		},
		{
			"date": "1946—1980",
			"description": "She was the initiator of the creation and the permanent chairman of the Belarusian Theater Society. Honorary Chairman since 1976."
